{
  "name": "wp-minimalist-boilerplate",
  "version": "1.0.0",
  "author": "João Vilaça",
  "description": "Minimal webpack boilerplate with HMR and sass",
  "repository": {
    "type": "git",
    "url": "https://github.com/jpggvilaca/webpack-boilerplate"
  },
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --colors",
    "prod": "webpack --config ./webpack/webpack.config.prod.js",
    "build": "webpack"
  },
  "keywords": [
    "webpack",
    "boilerplate",
    "hmr",
    "sass"
  ],
  "browserslist": [
    "> 1%",
    "not IE 11",
    "last 2 versions"
  ],
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.6.1",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^2.1.1",
    "html-webpack-plugin": "^5.3.1",
    "node-sass": "^4.14.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "webpack": "^5.32.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "mini-css-extract-plugin": "^1.4.1"
  }
